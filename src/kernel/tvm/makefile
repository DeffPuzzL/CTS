include $(BATHOME)/mak/platform.mak
include $(BATHOME)/mak/database.mak
include $(BATHOME)/mak/tuxedo.mak

PRGOBJS = tree.o sem.o msg.o tcp.o str.o list.o
        
PRGTARG = stvm
PRGDEFS = 

# used for db2 database
PRGDBNM = $(DBLINK)
PRGLIBS = -lxml2 $(NETLIBS) $(UXLIBS)

debug all: debugstatic
release: releasestatic

# DO NOT modify any code below!!!

releasedynamic debugdynamic releasestatic debugstatic releaseexec debugexec clean:
	@make -f $(HOME)/mak/mkstand.mak $@ TARGET="$(PRGTARG)" OBJS="$(PRGOBJS)" LIBS="$(PRGLIBS)" DEFS="$(PRGDEFS)" DBNM="$(PRGDBNM)"
